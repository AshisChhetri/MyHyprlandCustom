{
    "workbench.startupEditor": "none",
    "window.menuBarVisibility": "toggle",
    "editor.guides.bracketPairs": "active",
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', 'monospace','GoMono Nerd Font Mono'",
    "editor.fontLigatures": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.minimap.enabled": false,
    "editor.fontSize": 16,
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.env.linux": {},
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "console-ninja.featureSet": "Community",
    "liveServer.settings.donotVerifyTags": true,
    "editor.wordWrap": "on",
    "winopacity.opacity": 100,
    "dec-to-hex-color-opacity.autoLaunchDelay": 5,

    "vscode-blur-linux.opacity": 60,
    "glassit.force_sway": true,
    "glassit.alpha": 150,
    "window.zoomLevel": 1,
    "emulator.emulatorPathLinux": "~/Android/Sdk/emulator",

}